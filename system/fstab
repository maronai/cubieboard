# UNCONFIGURED FSTAB FOR BASE SYSTEM
/swap/swapfile swap swap defaults 0 0
tmpfs    	/tmp    	tmpfs	defaults		0	0
tmpfs    	/var/tmp	tmpfs	defaults		0 	0
/dev/sda1	/mnt/1		ext4	defaults,noauto		0	0
/dev/sda2	/mnt/2		auto	defaults,barrier=1	1	1
/dev/nanda	/mnt/a		auto	defaults		0	0
/dev/nandb	/mnt/b		auto	defaults		0	0
/dev/mmcblk0p1  /mnt/sd1        auto    defaults        	0       0
/dev/mmcblk0p2  /mnt/sd2        auto    defaults        	0       0
